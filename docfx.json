{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "JumpStart/bin/Release/net10.0/JumpStart.dll",
            "JumpStart/bin/Debug/net10.0/JumpStart.dll"
          ]
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "**.md",
          "toc.yml"
        ],
        "src": "docs",
        "dest": "."
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "styles/**"
        ],
        "src": "docs"
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "JumpStart Framework",
      "_appName": "JumpStart",
      "_appLogoPath": "images/logo.svg",
      "_appFaviconPath": "images/logo.svg",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_disableContribution": false,
      "_gitContribute": {
        "repo": "https://github.com/cyberknet/JumpStart",
        "branch": "main"
      },
      "_extraCss": "styles/custom.css",
      "_gitUrlPattern": "github"
    },
    "fileMetadata": {
      "_appFooter": {
        "**/*.md": "Copyright 2026 Scott Blomfield. Licensed under GPL-3.0."
      }
    }
  }
}
